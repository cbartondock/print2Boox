{
  "name": "Print to Boox",
  "version": "0.0.1",
  "description": "Print any page to Boox reader (via boox offical 'send2boox.com')",
  "options_page": "options.html",
  "background": {
    "scripts": ["aliyun-oss-sdk-6.0.0.min.js", "background.js"],
    "persistent": true
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    }
  },
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "permissions": [ "activeTab", "tabs", "declarativeContent", "http://*/*", "https://*/*", "printerProvider", "notifications", "storage", "unlimitedStorage" ],
  
  "manifest_version": 2
}
